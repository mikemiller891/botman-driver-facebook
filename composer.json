{
    "name": "botman/driver-facebook",
    "license": "MIT",
    "description": "Facebook Messenger driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Facebook Messenger",
        "Facebook",
        "Messenger"
    ],
    "homepage": "http://github.com/botman/driver-facebook",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "botman/botman": "dev-laravel7"
    },
    "require-dev": {
        "botman/studio-addons": "dev-laravel7",
        "phpunit/phpunit": "^8.5",
        "illuminate/contracts": "~5.5.0",
        "mockery/mockery": "dev-master",
        "ext-curl": "*",
        "symfony/http-foundation": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Facebook\\Providers\\FacebookServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mikemiller891/botman-botman.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikemiller891/botman-studio-addons.git"
        },
    ]
}
